{
  "name": "learner-model-gql-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "concurrently -r next pnpm:gql:watch pnpm:open:browser",
    "gql": "graphql-codegen --config codegen.yaml",
    "gql:watch": "graphql-codegen --config codegen.yaml --watch",
    "open:browser": "wait-on -s 1 http://localhost:3000 && open-cli http://localhost:3000",
    "pretty": "prettier --write \"**/*.{ts,tsx,json}\"",
    "start": "next start"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "@chakra-ui/react": "^1.7.3",
    "@chakra-ui/system": "^1.8.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/core": "^2.3.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "framer-motion": "^5.5.5",
    "graphql": "^16.2.0",
    "next": "^12.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.5",
    "rq-gql": "^0.3.1",
    "serialize-error": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/schema-ast": "^2.4.0",
    "@types/concurrently": "^6.4.0",
    "@types/node": "^16.11.14",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^6.5.1",
    "open-cli": "^7.0.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "wait-on": "^6.0.0"
  }
}
